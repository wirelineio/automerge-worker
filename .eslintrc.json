{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "jest": true,
    "worker": true,
    "shared-node-browser": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": "off",
    "consistent-return": "off",
    "import/prefer-default-export": "off",
    "max-len": [2, 120, 2, {
      "ignoreComments": true,
      "ignoreStrings": true,
      "ignoreTemplateLiterals": true,
      "ignoreUrls": true
      }
    ],
    "no-console": "off",
    "no-underscore-dangle": "off",
    "object-curly-newline": "off",
    "padded-blocks": "off"
  }
}
